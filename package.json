{
	"name": "@xibakon/stratos",
	"version": "1.0.0",
	"description": "FOR XIBAKON ONLY. PERSONAL PROJECT",
	"author": "xibakon @XiBakon",
	"bin": {
		"stratos": "./bin/run"
	},
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/bakon-projects/stratos_cli",
	"license": "MIT",
	"main": "dist/index.js",
	"repository": "bakon-projects/stratos_cli",
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"dependencies": {
		"@oclif/plugin-help": "^5",
		"@oclif/plugin-plugins": "^2.0.1",
		"@octokit/auth-basic": "^1.4.8",
		"@xibakon/cli-core": "^1.3.0",
		"adm-zip": "^0.5.9",
		"chalk": "^4.1.2",
		"chalk-animation": "^1.6.0",
		"conf": "^10.1.1",
		"configstore": "^6.0.0",
		"figlet": "^1.5.2",
		"gradient-string": "^2.0.2",
		"inquirer": "^8.2.0",
		"nanospinner": "^1.0.0",
		"node-fetch": "^2.6.7",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@oclif/test": "^2",
		"@types/adm-zip": "^0.4.34",
		"@types/chai": "^4",
		"@types/chalk-animation": "^1.6.1",
		"@types/cli-progress": "^3.9.2",
		"@types/ejs": "^3.1.0",
		"@types/figlet": "^1.5.4",
		"@types/fs-extra": "^9.0.13",
		"@types/gradient-string": "^1.1.2",
		"@types/inquirer": "^8.2.0",
		"@types/js-yaml": "^4.0.5",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.4",
		"@types/node-fetch": "^3.0.3",
		"@types/semver": "^7.3.9",
		"@types/wrap-ansi": "^8.0.1",
		"abort-controller": "^3.0.0",
		"chai": "^4",
		"fetch-blob": "^3.1.4",
		"globby": "^11",
		"mocha": "^9",
		"oclif": "^2",
		"shx": "^0.3.3",
		"ts-node": "^10.2.1",
		"tsd": "^0.19.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"oclif": {
		"bin": "stratos",
		"dirname": "stratos",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-plugins"
		],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			},
			"app": {
				"description": "Create a new React or Next App"
			},
			"smokinbs": {
				"description": "Download smokinbs files from source"
			}
		}
	},
	"scripts": {
		"build": "rimraf dist && tsc -b",
		"postpack": "shx rm -f oclif.manifest.json",
		"prepack": "npm run build && oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"bugs": "https://github.com/bakon-projects/stratos_cli/issues",
	"keywords": [
		"oclif"
	],
	"types": "dist/index.d.ts"
}
